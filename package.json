{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "prepare": "husky || echo 'Husky not installed'",
    "postinstall": "nuxt prepare",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "test": "playwright test",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:check": "vitest run --coverage",
    "test:coverage:ui": "vitest --coverage --ui",
    "test:quick": "vitest run --changed",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:chromium": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:webkit": "playwright test --project=webkit",
    "test:mobile": "playwright test --project=mobile",
    "test:auth": "playwright test tests/e2e/auth.spec.ts",
    "test:products": "playwright test tests/e2e/products.spec.ts",
    "test:checkout": "playwright test tests/e2e/checkout.spec.ts",
    "test:i18n": "playwright test tests/e2e/i18n.spec.ts",
    "test:visual": "playwright test tests/visual",
    "test:visual:update": "playwright test tests/visual --update-snapshots",
    "test:report": "playwright show-report",
    "test:setup": "playwright install --with-deps"
  },
  "dependencies": {
    "@nuxt/image": "^1.11.0",
    "@nuxtjs/i18n": "^10.0.3",
    "@pinia/nuxt": "^0.11.2",
    "@stripe/stripe-js": "^7.9.0",
    "@tailwindcss/vite": "^4.1.12",
    "@tanstack/vue-table": "^8.21.3",
    "@types/uuid": "^10.0.0",
    "@vite-pwa/nuxt": "^1.0.4",
    "@vueuse/core": "^13.9.0",
    "chart.js": "^4.5.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-vue-next": "^0.542.0",
    "nuxt": "^3.17.7",
    "reka-ui": "^2.5.1",
    "resend": "^6.0.1",
    "shadcn-nuxt": "^2.2.0",
    "stripe": "^18.5.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "vue-sonner": "^2.0.8",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.19.2",
    "@nuxtjs/supabase": "^1.6.0",
    "@pinia/testing": "^1.0.2",
    "@playwright/test": "^1.54.2",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "3.2.4",
    "@vue/test-utils": "^2.4.6",
    "dotenv": "^17.2.1",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "playwright": "^1.55.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=22.4.0",
    "pnpm": ">=9.0.0"
  },
  "pnpm": {}
}
