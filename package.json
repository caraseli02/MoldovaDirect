{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "prepare": "husky || echo 'Husky not installed'",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:oxlint": "oxlint --deny-warnings",
    "lint:fast": "oxlint --deny-warnings && eslint .",
    "lint:all": "pnpm lint:oxlint && pnpm lint && pnpm typecheck",
    "lint:knip": "knip",
    "lint:knip:fix": "knip --fix",
    "lint:staged": "lint-staged",
    "typecheck": "nuxi typecheck",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "test": "PLAYWRIGHT_TEST=true playwright test",
    "test:pre-commit": "PLAYWRIGHT_TEST=true playwright test --project=pre-commit",
    "test:critical": "PLAYWRIGHT_TEST=true playwright test --project=critical",
    "test:critical:headed": "PLAYWRIGHT_TEST=true playwright test --project=critical --headed",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:integration": "vitest run --config vitest.config.integration.ts",
    "test:integration:watch": "vitest --config vitest.config.integration.ts",
    "test:coverage": "vitest run --coverage",
    "test:coverage:check": "vitest run --coverage",
    "test:coverage:ui": "vitest --coverage --ui",
    "test:quick": "vitest run --changed",
    "test:ui": "PLAYWRIGHT_TEST=true playwright test --ui",
    "test:headed": "PLAYWRIGHT_TEST=true playwright test --headed",
    "test:debug": "PLAYWRIGHT_TEST=true playwright test --debug",
    "test:chromium": "PLAYWRIGHT_TEST=true playwright test --project=chromium",
    "test:firefox": "PLAYWRIGHT_TEST=true playwright test --project=firefox",
    "test:webkit": "PLAYWRIGHT_TEST=true playwright test --project=webkit",
    "test:mobile": "PLAYWRIGHT_TEST=true playwright test --project=mobile",
    "test:auth": "PLAYWRIGHT_TEST=true playwright test tests/e2e/auth.spec.ts",
    "test:products": "PLAYWRIGHT_TEST=true playwright test tests/e2e/products.spec.ts",
    "test:checkout": "PLAYWRIGHT_TEST=true playwright test tests/e2e/checkout.spec.ts",
    "test:checkout-flow": "PLAYWRIGHT_TEST=true playwright test tests/e2e/checkout-full-flow.spec.ts",
    "test:checkout-flow:headed": "PLAYWRIGHT_TEST=true playwright test tests/e2e/checkout-full-flow.spec.ts --headed",
    "test:i18n": "PLAYWRIGHT_TEST=true playwright test tests/e2e/i18n.spec.ts",
    "test:visual": "PLAYWRIGHT_TEST=true playwright test --config=playwright.visual-regression.config.ts",
    "test:visual:update": "PLAYWRIGHT_TEST=true playwright test --config=playwright.visual-regression.config.ts --update-snapshots",
    "test:visual-review": "PLAYWRIGHT_TEST=true playwright test --config=playwright.visual-regression.config.ts --ui",
    "test:visual:orders": "PLAYWRIGHT_TEST=true playwright test tests/e2e/visual/orders.spec.ts --project=chromium-es",
    "test:visual:products": "PLAYWRIGHT_TEST=true playwright test tests/e2e/visual/products-catalog.spec.ts --project=chromium-es",
    "test:visual:all": "PLAYWRIGHT_TEST=true playwright test tests/e2e/visual/ --project=chromium-es",
    "visual:serve": "node .visual-testing/serve.mjs",
    "visual:reset": "node .visual-testing/reset.mjs --force",
    "visual:check-approvals": "node .visual-testing/check-approvals.mjs",
    "test:report": "playwright show-report",
    "test:setup": "playwright install --with-deps",
    "setup:project": "node scripts/setup-github-project.mjs",
    "docs:check-links": "markdown-link-check docs/**/*.md README.md DOCUMENTATION_INDEX.md QUICK_START_GUIDE.md",
    "docs:lint": "markdownlint docs/**/*.md *.md --ignore node_modules --ignore .nuxt --ignore .output",
    "docs:find-outdated": "find docs -name '*.md' -mtime +180 -type f",
    "docs:audit": "pnpm docs:check-links && pnpm docs:lint",
    "docs:cli": "npx tsx scripts/documentation/cli.ts",
    "test:docs": "vitest run --config scripts/documentation/vitest.config.ts",
    "test:docs:watch": "vitest --config scripts/documentation/vitest.config.ts",
    "test:docs:coverage": "vitest run --coverage --config scripts/documentation/vitest.config.ts",
    "verify": "./scripts/verify-build.sh",
    "build:verify": "npm run build && npm run verify",
    "deploy:check": "npm run build:verify && echo \"âœ… Ready to deploy to Vercel\"",
    "test:e2e": "playwright test --config=playwright.cart.config.ts",
    "test:e2e:ui": "playwright test --config=playwright.cart.config.ts --ui",
    "test:e2e:headed": "playwright test --config=playwright.cart.config.ts --headed",
    "test:e2e:ci": "playwright test --config=playwright.cart.config.ts --project=chromium"
  },
  "dependencies": {
    "@nuxt/image": "^2.0.0",
    "@pinia/nuxt": "^0.11.3",
    "@stripe/stripe-js": "^8.6.4",
    "@tanstack/vue-table": "^8.21.3",
    "@types/uuid": "^11.0.0",
    "@vueuse/core": "^14.1.0",
    "@vueuse/motion": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "ipx": "^3.1.1",
    "jsonwebtoken": "^9.0.3",
    "lucide-vue-next": "^0.563.0",
    "pinia": "^3.0.4",
    "reka-ui": "^2.7.0",
    "resend": "^6.8.0",
    "shadcn-nuxt": "^2.4.3",
    "sharp": "^0.34.5",
    "stripe": "^20.2.0",
    "swiper": "^12.0.3",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^13.0.0",
    "vue": "3.5.27",
    "vue-router": "^4.6.4",
    "vue-sonner": "^2.0.9",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@nuxt/eslint": "^1.13.0",
    "@nuxt/test-utils": "^3.23.0",
    "@nuxtjs/i18n": "^10.2.1",
    "@nuxtjs/supabase": "^2.0.3",
    "@pinia/testing": "^1.0.3",
    "@playwright/test": "^1.58.0",
    "@supabase/supabase-js": "^2.91.1",
    "@tailwindcss/vite": "^4.1.18",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mdast": "^4.0.4",
    "@types/node": "^25.0.10",
    "@vite-pwa/nuxt": "^1.1.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "3.2.4",
    "@vue/test-utils": "^2.4.6",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-plugin-oxlint": "^1.41.0",
    "eslint-plugin-sonarjs": "^3.0.5",
    "fast-check": "^4.5.3",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "knip": "^5.82.1",
    "lint-staged": "^16.2.7",
    "markdown-link-check": "^3.14.2",
    "markdownlint-cli": "^0.47.0",
    "nuxt": "^4.2.2",
    "nuxt-swiper": "1.2.0",
    "oxlint": "^1.41.0",
    "playwright": "^1.58.0",
    "remark": "^15.0.1",
    "remark-parse": "^11.0.0",
    "terser": "^5.46.0",
    "typescript": "^5.9.3",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.1.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=22.4.0",
    "pnpm": ">=9.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": [
      "oxlint --deny-warnings",
      "eslint --fix"
    ],
    "*.{js,mjs,cjs}": [
      "oxlint --deny-warnings"
    ]
  },
  "pnpm": {
    "overrides": {
      "glob@>=10.2.0 <10.5.0": ">=10.5.0",
      "node-forge@<1.3.2": ">=1.3.2",
      "js-yaml@>=4.0.0 <4.1.1": ">=4.1.1",
      "js-yaml@<3.14.2": ">=3.14.2",
      "jws@<3.2.3": ">=3.2.3",
      "vite@>=7.1.0 <=7.1.10": ">=7.1.11",
      "vue": "3.5.26",
      "@vue/runtime-core": "3.5.26",
      "@vue/runtime-dom": "3.5.26",
      "@vue/server-renderer": "3.5.26",
      "@vue/shared": "3.5.26",
      "@vue/reactivity": "3.5.26",
      "@vue/compiler-sfc": "3.5.26",
      "@vue/compiler-dom": "3.5.26"
    }
  }
}
